# AI Provider 配置
VITE_AI_PROVIDER=openai  # 可选：openai, claude, kimi

# OpenAI 配置
VITE_OPENAI_API_KEY=your-api-key-here
VITE_OPENAI_MODEL=gpt-3.5-turbo
VITE_OPENAI_BASE_URL=your-base-url-here

# Claude 配置
VITE_CLAUDE_API_KEY=your-claude-key-here
VITE_CLAUDE_BASE_URL=your-claude-url-here

# Kimi 配置
VITE_KIMI_API_KEY=your-kimi-key-here
VITE_KIMI_BASE_URL=your-base-url-here

# deepseek配置
VITE_DEEPSEEK_API_KEY=your-deepseek-key-here
VITE_DEEPSEEK_BASE_URL=your-base-url-here

# 聊天配置
VITE_MAX_MESSAGE_LENGTH=1000
VITE_MAX_HISTORY_LENGTH=50

# 主题配置
VITE_DEFAULT_THEME=light

# 上传配置
VITE_MAX_UPLOAD_SIZE=5242880  # 5MB
VITE_ALLOWED_FILE_TYPES=image/png,image/jpeg,application/pdf 